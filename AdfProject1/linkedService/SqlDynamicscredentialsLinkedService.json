{
	"name": "SqlDynamicscredentialsLinkedService",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"LoginNameParameter": {
				"type": "String",
				"defaultValue": "etl_adf_user"
			},
			"LoginPasswordParameter": {
				"type": "String",
				"defaultValue": "qazwsx#123"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"server": "azsource-oltp-sql.database.windows.net",
			"database": "azsourceSQLdb",
			"encrypt": "mandatory",
			"trustServerCertificate": false,
			"authenticationType": "SQL",
			"userName": "@{linkedService().LoginNameParameter}",
			"password": "@{linkedService().LoginPasswordParameter}"
		}
	}
}